{
    "name": "boardingparty",
    "env": {
        "HTTP_SERVER_PORT": {
            "description": "Sets the port of pierrezemb/gostatic to be compatable with Cloud Run",
            "value": "8080"
        }
    },
    "build": {
        "skip": true
    },
    "hooks": {
        "prebuild": {
            "commands": [
                "echo 'prebuild'",
                "./fixTagRequirement"
            ]
        },
        "postbuild": {
            "commands": [
                "echo 'postbuild'",
                "./sync"
            ]
        },
        "precreate": {
            "commands": [
                "echo 'precreate'"
            ]
        },
        "postcreate": {
            "commands": [
                "echo 'postcreate'"
            ]
        }
    }
}